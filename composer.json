{
  "name": "xdimedrolx/name-case-lib",
  "type": "library",
  "description": "Library for inflecting Russian and Ukrainian Names",
  "keywords": [
    "inflector",
    "NameCaseLib"
  ],
  "authors": [
    {
      "name": "Andriy Chaika",
      "email": "andriy@chaika.in.ua",
      "role": "Core developer"
    },
    {
      "name": "Rabotyahoff",
      "homepage": "https://github.com/Rabotyahoff",
      "role": "Core developer"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/seagullua/NameCaseLib",
  "support": {
    "issues": "https://github.com/seagullua/NameCaseLib/issues"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "NCL\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NCLTest\\": "tests/Library"
    }
  },
  "scripts": {
    "test": "phpunit --coverage-text; php-cs-fixer fix -v --diff --dry-run --allow-risky=yes;",
    "phpunit": "phpunit --coverage-text",
    "phpcs": "php-cs-fixer fix -v --diff --allow-risky=yes;"
  }
}
